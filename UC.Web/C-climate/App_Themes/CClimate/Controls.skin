   <RowStyle BackColor="#f1f4f5" />
   <FieldHeaderStyle BackColor="#f1f4f5" Font-Bold="True" />
</asp:DetailsView>

<asp:DataList runat="server" Width="100%" CssClass="alltable" GridLines="None">
</asp:DataList>

