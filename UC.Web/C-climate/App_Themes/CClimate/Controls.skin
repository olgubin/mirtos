   <RowStyle BackColor="Transparent" />
   <FieldHeaderStyle BackColor="White"/>
</asp:DetailsView>

<asp:DetailsView runat="server" CellPadding="4" ForeColor="#333333" GridLines="None" Height="50px" Width="100%">
